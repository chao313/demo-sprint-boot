<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="demo.spring.boot.demospringboot.mybatis.dao.CatDAO">

    <select id="queryById" parameterType="java.lang.Integer"
            resultType="demo.spring.boot.demospringboot.mybatis.vo.Cat">
        SELECT id , name , age from t_cat WHERE id = #{catId}

   </select>

    <update id="updateById" parameterType="demo.spring.boot.demospringboot.mybatis.vo.Cat">

        UPDATE t_cat set name = #{name} , age = #{age} WHERE id = #{id}

    </update>

    <insert id="insert" parameterType="demo.spring.boot.demospringboot.mybatis.vo.Cat">
         insert t_cat (name , age) values(#{name} , #{age})
    </insert>

    <delete id="deleteById" parameterType="java.lang.Integer">
         DELETE FROM t_cat WHERE  id = #{catId}
    </delete>
</mapper>